ROOT         = {"tables": TABLE_LIST}
TABLE_LIST   = [TABLE*]
TABLE        = {TABLE_ID, TABLE_DEF}
TABLE_ID     = string
TABLE_DEF    = [TABLE_ENTRY*]
TABLE_ENTRY  = [ENTRY_WEIGHT, ENTRY_TYPE, ENTRY_ID, ENTRY_AMOUNT]
ENTRY_WEIGHT = int
ENTRY_TYPE   = string
ENTRY_ID     = string
ENTRY_AMOUNT = DISTRIBUTION | null
DISTRIBUTION = DIST_UNIFORM | DIST_NORMAL | DIST_FIXED
DIST_UNIFORM = ["uniform", MIN, MAX]
DIST_NORMAL  = ["normal", MEAN, STDDEV]
DIST_FIXED   = ["fixed", VALUE]
MIN          = int
MAX          = int
MEAN         = int
STDDEV       = int
VALUE        = int

/*******************************************************************************
TABLE_ID
--A unique, among all other tables, string id for the table; must also hash uniquely.

TABLE_ENTRY
--A list of entries that defines a discrete_distribution.

ENTRY_WEIGHT
--A positive integral relative weight for the discrete_distribution.

ENTRY_TYPE
--"item"  - ENTRY_ID refers to an item.
--"table" - ENTRY_ID refers to another table.

ENTRY_ID
--The string id of an item or table.

ENTRY_AMOUNT
--null         - always 1.
--DISTRIBUTION - a random amount given by DISTRIBUTION.

DIST_UNIFORM
--A uniform random integer from [MIN, MAX].

DIST_NORMAL
--A random integer from the normal distribution with mean = MEAN and standard deviation = STDDEV.

DIST_FIXED
--A fixed integral value.
/******************************************************************************/