ROOT              = {"items": ITEM_LIST}
ITEM_LIST         = [ITEM*]
ITEM              = {ITEM_ID, ITEM_CATEGORY, ITEM_TYPE, ITEM_TAG_LIST, ITEM_ATTR_LIST, ITEM_BASE_VALUE, ITEM_NAME, ITEM_DESC}
ITEM_ID           = string
ITEM_CATEGORY     = string
ITEM_TYPE         = string
ITEM_TAG_LIST     = [ITEM_TAG*]
ITEM_TAG          = string
ITEM_BASE_VALUE   = int
ITEM_ATTR_LIST    = [ITEM_ATTR*]
ITEM_ATTR         = [ATTR_NAME, ATTR_VALUE]
ATTR_NAME         = string
ATTR_VALUE        = any
ITEM_NAME         = LANG_STRING_LIST
ITEM_DESC         = LANG_STRING_LIST
LANG_STRING_LIST  = [LANG_STRING*]
LANG_STRING       = [LANG_STRING_ID, LANG_STRING_VALUE]
LANG_STRING_ID    = string
LANG_STRING_VALUE = string

/*******************************************************************************
TABLE_ID
--A unique, among all other tables, string id for the table; must also hash uniquely.

TABLE_ENTRY
--A list of entries that defines a discrete_distribution.

ENTRY_WEIGHT
--A positive integral relative weight for the discrete_distribution.

ENTRY_TYPE
--"item"  - ENTRY_ID refers to an item.
--"table" - ENTRY_ID refers to another table.

ENTRY_ID
--The string id of an item or table.

ENTRY_AMOUNT
--null         - always 1.
--DISTRIBUTION - a random amount given by DISTRIBUTION.

DIST_UNIFORM
--A uniform random integer from [MIN, MAX].

DIST_NORMAL
--A random integer from the normal distribution with mean = MEAN and standard deviation = STDDEV.

DIST_FIXED
--A fixed integral value.
/******************************************************************************/